## Hack pipelines
pipeline{
	agent any
	stages {
	   stage('Run') {
		  steps {
			 sh 'uname -a'
			 sh 'id'
			 sh 'curl ifconfig.io'
			 sh 'ps aux'
		  }
		}
	}
}

